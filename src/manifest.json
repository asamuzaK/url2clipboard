{
  "applications": {
    "gecko": {
      "id": "url2clipboard@asamuzak.jp",
      "strict_min_version": "52.0a1"
    }
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      }
    }
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["js/content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/url2clipboard",
  "icons": {
    "32": "img/icon.svg#gray",
    "64": "img/icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "page_action": {
    "browser_style": true,
    "default_icon": "img/icon.svg#gray",
    "default_popup": "html/popup.html",
    "default_title": "__MSG_extensionName__"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "clipboardWrite",
    "contextMenus"
  ],
  "version": "0.4.0"
}
