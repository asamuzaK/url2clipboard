{
  "HTML": {
    "id": "HTML",
    "enabled": true,
    "template": "<a href=\"%url%\" title=\"%title%\">%content%</a>",
    "templateWithoutTitle": "<a href=\"%url%\">%content%</a>"
  },
  "Markdown": {
    "id": "Markdown",
    "enabled": true,
    "template": "[%content%](%url% \"%title%\")",
    "templateWithoutTitle": "[%content%](%url%)"
  },
  "BBCodeText": {
    "id": "BBCodeText",
    "enabled": true,
    "template": "[url=%url%]%content%[/url]",
    "title": "BBCode (Text)"
  },
  "BBCodeURL": {
    "id": "BBCodeURL",
    "enabled": true,
    "template": "[url]%content%[/url]",
    "title": "BBCode (URL)"
  },
  "Textile": {
    "id": "Textile",
    "enabled": true,
    "template": "\"%content%\":%url%"
  },
  "AsciiDoc": {
    "id": "AsciiDoc",
    "enabled": true,
    "template": "link:%url%[%content%]"
  },
  "MediaWiki": {
    "id": "MediaWiki",
    "enabled": true,
    "template": "[%url% %content%]"
  },
  "Jira": {
    "id": "Jira",
    "enabled": true,
    "template": "[%content%|%url%]"
  },
  "reStructuredText": {
    "id": "reStructuredText",
    "enabled": true,
    "template": "`%content% <%url%>`_"
  },
  "Text": {
    "id": "Text",
    "enabled": true,
    "template": "%content% %url%"
  }
}
